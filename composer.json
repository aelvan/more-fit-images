{
    "name": "aelvan/more-fit-images",
    "description": "Fixes a bug in Craft 3.4 where thumbs in the Control Panel were cropped instead of fitted.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "more fit images"
    ],
    "support": {
        "docs": "https://github.com/aelvan/more-fit-images/blob/master/README.md",
        "issues": "https://github.com/aelvan/more-fit-images/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√© Elvan",
            "homepage": "https://www.vaersaagod.no/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
          "aelvan\\morefitimages\\": "src/"
        }
    },
    "extra": {
        "name": "More Fit Images",
        "handle": "more-fit-images",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/aelvan/more-fit-images/master/CHANGELOG.md",
        "class": "aelvan\\morefitimages\\MoreFitImages"
    }
}
